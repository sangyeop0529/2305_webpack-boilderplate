{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "useTabs": false,
        "endOfLine": "auto"
      }
    ],
    "import/prefer-default-export": "off",
    "no-new": "off",
    "no-console": "off",
    "class-methods-use-this": "off"
  }
}
